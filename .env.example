# Environment Variables for NutriWell.ai
# Copy this file to .env.local and fill in your actual values

# Application Configuration
NEXT_PUBLIC_APP_NAME=NutriWell.ai
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_DESCRIPTION="Smart AI-powered nutrition assistant platform"

# Analytics (Optional)
# NEXT_PUBLIC_GA_TRACKING_ID=
# NEXT_PUBLIC_HOTJAR_ID=

# Authentication (When implemented)
# NEXTAUTH_URL=http://localhost:3000
# NEXTAUTH_SECRET=your-secret-key-here
# GITHUB_CLIENT_ID=
# GITHUB_CLIENT_SECRET=
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=

# Database (When implemented)
# DATABASE_URL=
# DATABASE_HOST=
# DATABASE_PORT=
# DATABASE_NAME=
# DATABASE_USER=
# DATABASE_PASSWORD=

# AI/API Configuration (When implemented)
# OPENAI_API_KEY=
# ANTHROPIC_API_KEY=
# NUTRITION_API_KEY=

# Email Configuration (When implemented)
# SMTP_HOST=
# SMTP_PORT=
# SMTP_USER=
# SMTP_PASSWORD=
# EMAIL_FROM=

# File Storage (When implemented)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=
# AWS_S3_BUCKET=

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_AUTH=false
NEXT_PUBLIC_ENABLE_CHAT=true

# Development
NODE_ENV=development
